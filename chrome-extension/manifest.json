{
  "name": "Image Translator",
  "description": "Translate text from image",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "hello_extensions.png"
  },
  "permissions": [
    "activeTab"
  ]
}